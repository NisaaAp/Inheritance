package inheritance;

/**
 *
 * @author User
 */
public class Person {
    private final String name;
    private final int usia;
    
    //Konstruktor
    public Person (String name, int usia){
        this.name = name;
        this.usia = usia;
    }
    
    //Metode
    public void info(){
        System.out.println("Nama : " + this.name);
        System.out.println("Usia : " + this.usia);
    }
            
}
