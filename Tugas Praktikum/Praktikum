package inheritance;

/**
 *
 * @author User
 */
public class TugasPraktikum {
    
    //attribut utama
    protected String nama, status, jenisKelamin, agama;
    protected int usia;
    
    //konstruktor
    public TugasPraktikum (String nama, String status, String agama, String jenisKelamin, int usia){
        this.nama = nama;
        this.status = status;
        this.agama = agama;
        this.jenisKelamin = jenisKelamin;
        this.usia = usia;
    }
    
    public void info(){
        System.out.println("Nama          : " + this.nama);
        System.out.println("Agama         : " + this.agama);
        System.out.println("Jenis Kelamin : " + this.jenisKelamin);
        System.out.println("Status        : " + this.status);
        System.out.println("Usia          : " + this.usia);
    }
}
